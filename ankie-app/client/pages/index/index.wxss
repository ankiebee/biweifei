/**index.wxss**/
page {
    background: #F6F6F6;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}
.landing-logo{
	margin-left:auto;
	margin-right:auto;
	margin-top:25px;
}
.welcome-text{
  font-family:'Raleway', sans-serif;
	font-size:20px;
	color:#535353;
	font-weight:300;
	text-align:center;
	text-transform:uppercase;
	margin-top:20px;
  margin-bottom:15px;
}

.navigation-icons{
	width:270px;
	margin-left:auto;
	margin-right:auto;
}

.nav-icon{
	float:left;
	border-radius:50px;
	width:60px;
	height:60px;
	margin-left:15px;
	margin-right:15px;
  margin-top:10px;
	margin-bottom:25px;
}
